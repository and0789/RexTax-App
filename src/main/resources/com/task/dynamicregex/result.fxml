<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/null"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.task.dynamicregex.controllers.ResultController">
   <Label fx:id="titleLabel" alignment="CENTER" layoutX="371.0" layoutY="30.0" prefHeight="86.0" styleClass="title"
          text="Title" textAlignment="CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0">
      <font>
         <Font name="Arial" size="40.0"/>
      </font>
      <effect>
         <DropShadow height="20.0" radius="9.5" width="20.0">
            <color>
               <Color opacity="0.5"/>
            </color>
         </DropShadow>
      </effect>
   </Label>
   <Label alignment="CENTER" contentDisplay="CENTER" layoutX="353.0" layoutY="127.0" text="Result List"
          textAlignment="CENTER" AnchorPane.topAnchor="123.0">
      <font>
         <Font name="Arial" size="20.0"/>
      </font>
   </Label>
   <TableView fx:id="resultTableView" layoutX="276.0" layoutY="150.0" prefHeight="300.0" AnchorPane.bottomAnchor="120.0"
              AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="150.0">
      <columns>
         <TableColumn fx:id="noTableColumn" maxWidth="40.0" minWidth="40.0" prefWidth="40.0" sortable="false"
                      styleClass="column-center" text="#"/>
         <TableColumn fx:id="fieldTableColumn" prefWidth="170.0" text="Field"/>
         <TableColumn fx:id="resultTableColumn" prefWidth="388.0" text="Result"/>
      </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
      </columnResizePolicy>
   </TableView>
   <Button fx:id="backButton" layoutX="100.0" layoutY="487.0" mnemonicParsing="false" onAction="#backButtonOnAction"
           prefHeight="40.0" text="Back" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="100.0">
      <font>
         <Font size="15.0"/>
      </font>
      <padding>
         <Insets left="20.0" right="20.0"/>
      </padding>
   </Button>
   <HBox layoutX="388.0" layoutY="487.0" spacing="20.0" AnchorPane.bottomAnchor="50.0" AnchorPane.rightAnchor="100.0"/>
</AnchorPane>
